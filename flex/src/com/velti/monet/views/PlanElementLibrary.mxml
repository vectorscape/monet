<?xml version="1.0" encoding="utf-8"?>
<controls:CollapsiblePanel 
	xmlns:mx="http://www.adobe.com/2006/mxml"
	xmlns:controls="com.velti.monet.controls.*"
	title="Plan Element Library"
	horizontalAlign="center" verticalGap="10"
	paddingTop="10">
	<mx:Script>
		<![CDATA[
			import com.velti.monet.models.Element;
			import com.velti.monet.models.ElementStatus;
			import com.velti.monet.models.ElementType;
		]]>
	</mx:Script>
	<controls:ElementRenderer element="{new Element(ElementType.AUDIENCE,'Audience',null,ElementStatus.COMPLETE,true)}" 
							  width="65" height="45" />
	<controls:ElementRenderer element="{new Element(ElementType.PUBLISHER,'Publisher',null,ElementStatus.COMPLETE,true)}" 
							  width="65" height="45"/>
	<controls:ElementRenderer element="{new Element(ElementType.PLACEMENT,'Placement',null,ElementStatus.COMPLETE,true)}" 
							  width="65" height="45"/>
	<controls:ElementRenderer element="{new Element(ElementType.AD,'Ad',null,ElementStatus.COMPLETE,true)}"
							  width="65" height="45"/>
</controls:CollapsiblePanel>
